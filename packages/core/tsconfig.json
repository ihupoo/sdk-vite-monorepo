{
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "./types",
        "target": "ES5",
        "module": "ESNext",
        "declaration": true,
        "noEmit": true,
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["./src"]
  }
